{
  "name": "famous-seed",
  "version": "0.4.2",
  "description": "seed for a browserified version of famous",
  "browser": "src/index.js",
  "scripts": {
    "build-main": "browserify src/main.js -g uglifyify | uglifyjs --screw-ie8 -m -c dead_code,sequences,conditionals,booleans,unused,if_return,join_vars,drop_debugger > public/main.bundle.js",
    "build-worker": "browserify src/main.js -g uglifyify | uglifyjs --screw-ie8 -m -c dead_code,sequences,conditionals,booleans,unused,if_return,join_vars,drop_debugger > public/worker.bundle.js",
    "build": "npm run build-main & npm run build-worker",
    "watch-main": "watchify src/main.js -o public/main.bundle.js -d -v",
    "watch-worker": "watchify src/worker.js -o public/worker.bundle.js -d -v",
    "watch": "npm run watch-main & npm run watch-worker",
    "start": "npm run build && serve --compress public/ -p 1337",
    "dev": "npm run watch & serve public/ -p 1337",
    "test": "npm run lint -s && npm run build",
    "lint-eslint": "eslint src/",
    "lint-jscs": "jscs src/",
    "lint": "npm run lint-eslint && npm run lint-jscs"
  },
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "devDependencies": {
    "browserify": "^10.1.3",
    "eslint": "^0.21.0",
    "jscs": "^1.7.3",
    "serve": "^1.4.0",
    "uglify-js": "^2.4.15",
    "watchify": "^3.1.0",
    "uglifyify": "^3.0.1"
  },
  "author": "Famous",
  "license": "All rights reserved",
  "dependencies": {
    "babelify": "^6.0.1",
    "famous": "git+ssh://git@github.com/Famous/mixed-mode#develop"
  }
}
